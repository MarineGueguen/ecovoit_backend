{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },

    "plugins": ["@typescript-eslint"],
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  
    "rules": {
      "@typescript-eslint/no-unused-vars": 2, // error ou 2: traiter la règle comme une erreur. Echouera lors de l'exécution d'un linter
      "@typescript-eslint/consistent-type-definitions": ["error", "type"]
    },
  
    "env": {
      "browser": true,
      "es2021": true
    }
}
